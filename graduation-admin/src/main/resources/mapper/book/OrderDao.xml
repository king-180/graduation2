<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.com.wx.modules.book.dao.OrderDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.com.wx.modules.book.entity.OrderEntity" id="orderMap">
        <result property="id" column="id"/>
        <result property="userId" column="user_id"/>
        <result property="money" column="money"/>
        <result property="createTime" column="creat_time"/>
        <result property="isDelete" column="is_delete"/>
        <result property="bookId" column="book_id"/>
    </resultMap>
    <insert id="insert" parameterType="com.com.wx.modules.book.entity.OrderEntity">
        INSERT INTO t_order (id, user_id, money, create_time, is_delete, book_id)
        VALUES (#{id}, #{userId}, #{money}, #{createTime}, #{isDelete}, #{bookId})
    </insert>


</mapper>